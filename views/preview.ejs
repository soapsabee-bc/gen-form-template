                <label for="<%= ele.id %>"><%= ele.name %></label>
                <input id="<%= ele.id %> " style="width: 100%;" type="date">

            <% } %> 

            <% if(ele.type === "group"  ) {%>
              <div class="jumbotron jumbotron-fluid">
                <div class="container">
                  <h1 class="display-4">Fluid jumbotron</h1>
                </div>
              </div>
          <% } %> 

          <% if(ele.type === "upload"  ) {%>
            <label for="<%= ele.id %>"><%= ele.name %></label>
            <input id="<%= ele.id %> " style="width: 100%;" type="file">

        <% } %> 
           
        <% if(ele.type === "readonly" ) {%>
          <label for="<%= ele.id %>"><%= ele.name %></label>
