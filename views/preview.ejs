                      <label for="<%= ele.id %>"><%= ele.name %></label>
                      <input id="<%= ele.id %> " style="width: 100%;" type="text">
                  
                  <% } %> 
                  <% if(ele.type === "table"  ) {%>
                  
                    <table class="table table-sm">
                      <thead>
                        <tr>
                          <th scope="col">#</th>
                          <th scope="col">First</th>
                          <th scope="col">Last</th>
                          <th scope="col">Handle</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <th scope="row">1</th>
                          <td>Mark</td>
                          <td>Otto</td>
                          <td>@mdo</td>
                        </tr>
